        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <include layout="@layout/keyguard_clock_ios"/>
    </LinearLayout>
    <LinearLayout
        android:gravity="center_vertical"
        android:id="@+id/keyguard_clock_style_cos"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <include layout="@layout/keyguard_clock_cos"/>
    </LinearLayout>
    <LinearLayout
        android:gravity="center_vertical"
        android:id="@+id/keyguard_clock_style_custom"
